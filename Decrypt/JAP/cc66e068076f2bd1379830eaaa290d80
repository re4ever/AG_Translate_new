function HelpData_Xls.GetExcelData(id)
	local data = HelpData_Xls.xls[tostring(id)]
	if CONST.LOG_DEBUG and data == nil then logWarn("Excel表 <color=yellow>HelpData_Xls</color> 找不到 ID: <color=yellow>"..tostring(id).."</color> 这只是个警告，帮助查错。没bug无视他! (By龙波)") end
	return data
end
return HelpData_Xls